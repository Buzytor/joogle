extends layout
block content
  include header
  #details
    .result
      .declaration
        h3
          a(href=details.link)= details.name
        ul.signatures
          each signature in details.signatures
            li= signature
      ul.tags
        each tag in details.tags
          li= tag
      p.description= details.description
      a(href=details.link).link= details.link
