extends layout
block content
  include header
  #search 
    #results
      each result in results
        .result
          h3
            a(href="details/" + result.name)
              span.name= result.name
              span.type_separator ::
              span.signature= result.signature
          p.description= result.description
            ul.tags
              each tag in result.tags
                li= tag
